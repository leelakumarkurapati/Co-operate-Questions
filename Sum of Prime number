def is_prime(num):
    if num <= 1:
        return False
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            return False
    return True
while True:
    m = int(input("Enter the value of m (0 to stop): "))
    if m == 0:
        break
    n = int(input("Enter the value of n: "))
    total = 0
    start, end = (m, n) if m <= n else (n, m)
        for number in range(start, end + 1):
        if is_prime(number):
            total += number
    print(f"Sum of prime numbers between {start} and {end} is: {total}")
