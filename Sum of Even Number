while True:
    m = int(input("Enter the value of m: "))
    if m == 0:
        break
    n = int(input("Enter the value of n: "))
    total = 0
    
    if m > n:
        for i in range(m, n - 1, -1): 
            if i % 2 == 0:            
                total += i
        print("Sum of even numbers from", m, "to", n, "is:", total)
    else:
        print("m should be greater than n")
